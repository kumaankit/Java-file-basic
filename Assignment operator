public class Assignmentoperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	int a,b;
	a=10;
	b =5;
	System.out.println(a);
	
	a+=10;// compound(a=a+10)
		System.out.println(a);
		

		a-=10;// compound(a=a-10)
			System.out.println(a);
	a=b;
	System.out.println(a);
	
	}

}
