public class Incrdecr {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a = 10;
		System.out.println(a);
		System.out.println("incr/decr value");
		System.out.println(--a); //pre
		System.out.println(a--);// post
		
		System.out.println(++a); // pre
		System.out.println(a++); // post
		System.out.println(a);
	}

}
